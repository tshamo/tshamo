                    ),
                    #podName#,#COUNT#
                  ),
                  GROUPBYTAG(
                    EXCLUDE(INCLUDE(
                      PROPAGATE(
                        $start:$end:synthetics.*.AGG.all:AVAILABILITY_METRIC_NAME{operationalStatus=ACTIVE,dataCenterName=$ALLDC,substrate=SFDC_CORE,podName=*,level=testCase,testCaseId=TESTCASEID}:max:10m-max,#10m#
